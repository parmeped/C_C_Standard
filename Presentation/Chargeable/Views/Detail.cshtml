@model Application.Chargeable.Queries.GetChargeableDetail.ChargeableDetailModel

@{
    ViewBag.Title = "ChargeableDetail";
}

@{
    Layout = "~/Shared/Views/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>
<table>
    <tr>
        <th>ID</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Value</th>
        <td>@Model.Value</td>
    </tr>
    <tr>
        <th>Active</th>
        <td>@Model.Active</td>
    </tr>

</table>
<div class="pull-right">
    @Html.ActionLink("Back to Categories", "Index", "categories")
</div>
